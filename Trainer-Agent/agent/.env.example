# Supabase Configuration
# URL of your Supabase project (Requirement 10.1)
SUPABASE_URL=https://your-project.supabase.co

# Supabase service role key for database authentication (Requirement 10.2)
SUPABASE_KEY=your-supabase-service-role-key

# Google Cloud Platform Configuration
# Name of the GCP bucket for storing datasets and models (Requirement 10.3)
GCP_BUCKET_NAME=your-gcp-bucket-name

# Path to GCP service account credentials JSON file (Requirement 10.4)
GOOGLE_APPLICATION_CREDENTIALS=/path/to/service-account-key.json

# Logging Configuration
# Log level for application logging (Requirement 10.5)
# Options: DEBUG, INFO, WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO

# Training Configuration (Optional)
# Default batch size for training
BATCH_SIZE=32

# Default number of training epochs
DEFAULT_EPOCHS=10

# Default learning rate for optimizer
DEFAULT_LEARNING_RATE=0.001
